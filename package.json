{
  "name": "@ts-stack/body-parser",
  "type": "module",
  "description": "Node.js body parser writen in TypeScript, in promise style, in ESM format, without support Node.js version < 20.6.0.",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "https://github.com/ts-stack/body-parser",
  "exports": "./dist/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "build-test": "tsc -b tsconfig.unit.json",
    "lint": "eslint .",
    "test": "npm run build-test && npm run esm-jest",
    "esm-jest": "node --env-file=.env --experimental-vm-modules --no-warnings=ExperimentalWarning --enable-source-maps node_modules/jest/bin/jest.js",
    "test-ci": "nyc --reporter=lcov --reporter=text npm src",
    "test-cov": "nyc --reporter=html --reporter=text npm src"
  },
  "dependencies": {
    "bytes": "3.1.2",
    "content-type": "~1.0.5",
    "debug": "4.3.5",
    "http-errors": "2.0.0",
    "iconv-lite": "0.6.3",
    "media-typer": "0.3.0",
    "mime-types": "~2.1.24",
    "qs": "6.12.3"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.4",
    "@types/content-type": "^1.1.8",
    "@types/debug": "^4.1.12",
    "@types/eslint": "^8.56.10",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/media-typer": "0.3.0",
    "@types/methods": "^1.1.4",
    "@types/mime-types": "~2.1.4",
    "@types/node": "^20.14.10",
    "@types/qs": "^6.9.15",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^9.7.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.4.0",
    "eslint-plugin-standard": "5.0.0",
    "jest": "^29.7.0",
    "methods": "1.1.2",
    "mocha": "10.6.0",
    "nyc": "17.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "safe-buffer": "5.2.1",
    "supertest": "7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">= 20.6.0"
  }
}
